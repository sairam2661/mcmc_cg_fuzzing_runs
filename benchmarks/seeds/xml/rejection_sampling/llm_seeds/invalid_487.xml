<?xml version='1.0' encoding='UTF-8'?>
<!DOCTYPE root [
  <!ELEMENT root (name, address, phone | phone, address, name)>
  <!ELEMENT name (#PCDATA)>
  <!ELEMENT address (street, city, state, zip)>
  <!ELEMENT street (#PCDATA)>
  <!ELEMENT city (#PCDATA)>
  <!ELEMENT state (#PCDATA)>
  <!ELEMENT zip (#PCDATA)>
  <!ELEMENT phone (#PCDATA)>
]>
<root>
  <phone>123-456-7890</phone>
  <address>
    <street>123 Main St</street>
    <city>New York</city>
    <state>NY</state>
    <zip>10001</zip>
  </address>
  <name>John Doe</name>
</root>