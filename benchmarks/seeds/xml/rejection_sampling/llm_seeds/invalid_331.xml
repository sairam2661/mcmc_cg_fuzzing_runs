<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE notes [
  <!ELEMENT notes (note+)>
  <!ELEMENT note (title, body)>
  <!ELEMENT title (#PCDATA)>
  <!ELEMENT body (#PCDATA)>
  <!ELEMENT author (#PCDATA)>
  <!ELEMENT date (#PCDATA)>
  <!ELEMENT category (#PCDATA)>
  <!ELEMENT address (street, city, state, zip)>
  <!ELEMENT street (#PCDATA)>
  <!ELEMENT city (#PCDATA)>
  <!ELEMENT state (#PCDATA)>
  <!ELEMENT zip (#PCDATA)>
  <!ELEMENT contact (phone, email)>
  <!ELEMENT phone (#PCDATA)>
  <!ELEMENT email (#PCDATA)>
  <!ELEMENT phonebook (phone+)>
  <!ELEMENT emailbook (email+)>
  <!ELEMENT phonebookref (#PCDATA)>
  <!ELEMENT emailbookref (#PCDATA)>
  <!ATTLIST note id CDATA #REQUIRED>
  <!ATTLIST note author CDATA #IMPLIED>
  <!ATTLIST note date CDATA #IMPLIED>
  <!ATTLIST note category CDATA #IMPLIED>
]>
<notes>
  <note id="1" author="John Doe" date="2022-01-01" category="Work">
