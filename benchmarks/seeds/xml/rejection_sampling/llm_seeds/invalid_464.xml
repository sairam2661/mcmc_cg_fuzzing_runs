<?xml version="1.0" encoding="ISO-8859-1"?>
<!DOCTYPE root [
  <!ELEMENT root (person, address, phone, creditcard, comment) >
  <!ELEMENT person (name, age, height, weight) >
  <!ELEMENT address (street, city, state, zip) >
  <!ELEMENT phone (number, type) >
  <!ELEMENT creditcard (number, type, expiration, cvv) >
  <!ELEMENT comment (#PCDATA) >
  <!ATTLIST root id CDATA #REQUIRED>
  <!ATTLIST person name CDATA #REQUIRED>
  <!ATTLIST address street CDATA #REQUIRED>
  <!ATTLIST phone number CDATA #REQUIRED>
  <!ATTLIST creditcard number CDATA #REQUIRED>
]>
<root id="123456789">
  <person name="John Doe">
    <age>32</age>
    <height>5'10"</height>
    <weight>150</weight>
  </person>
  <address street="123 Main St">
    <city>New York</city>
    <state>NY</state>
    <zip>10001</zip>
  </address>
  <phone number="555-555-5555">
    <type>cell</type>
  </phone>
  <creditcard number="1234-5678-9012-3456">
    <type>visa</type>
    <expiration>02/2025</expiration>
    <cvv>123</cvv>
  </creditcard>
  <comment>This is a comment</comment>
</root> 
