<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE note SYSTEM "note.dtd">
<note xmlns:dc="http://purl.org/dc/elements/1.1/"
      xmlns:xlink="http://www.w3.org/1999/xlink"
      xmlns="http://www.w3.org/2003/05/soap-envelope/"
      xmlns:ex="http://example.org/namespace">
  <to>John</to>
  <from>Joe</from>
  <message>
    <p>Hello!</p>
    <p>How are you?</p>
  </message>
  <dc:creator>John Smith</dc:creator>
  <ex:comment>This is a comment</ex:comment>
  <link xlink:href="http://www.example.com" xlink:title="Example"/>
  <ex:element>
    <ex:child/>
    <ex:grandchild/>
  </ex:element>
</note>