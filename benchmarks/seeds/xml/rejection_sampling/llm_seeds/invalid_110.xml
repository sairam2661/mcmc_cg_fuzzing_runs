<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE note SYSTEM "note.dtd">
<!ELEMENT note (to,from,heading,body)>
<!ELEMENT to (#PCDATA)>
<!ELEMENT from (#PCDATA)>
<!ELEMENT heading (#PCDATA)>
<!ELEMENT body (#PCDATA)>

<?xml-stylesheet type="text/xsl" href="note.xsl"?>
<?xml:lang="en"?>
<?xml:space="preserve"?>
<?xml:lang="en"?>

<note>
  <to>Alice</to>
  <from>Bob</from>
  <heading>Meeting Tomorrow</heading>
  <body>
    <p>
      Please attend the meeting tomorrow at 2 PM.
    </p>
    <p>
      We will discuss the new project.
    </p>
  </body>
</note>