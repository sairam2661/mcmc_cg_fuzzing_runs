<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE document SYSTEM "http://example.com/schema.dtd">
<document xmlns:xs="http://www.w3.org/2001/XMLSchema-instance"
          xs:noNamespaceSchemaLocation="schema.xsd"
          xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
          xsi:schemaLocation="http://example.com/schema.xsd schema.xsd">
  <person>
    <name>John</name>
    <address>
      <street>123 Main St</street>
      <city>New York</city>
      <state>NY</state>
      <zip>10001</zip>
    </address>
    <phone>
      <type>home</type>
      <number>555-1234</number>
    </phone>
  </person>
  <person>
    <name>Jane</name>
    <address>
      <street>456 Elm St</street>
      <city>Chicago</city>
      <state>IL</state>
      <zip>60601</zip>
    </address>
    <phone>
      <type>work</type>
      <number>555-5678</number>
    </phone>
  </person>
  <comment>
    <text>This is a comment</text>
  </comment>
</document>