<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE root [
  <!ELEMENT root (person+)>
  <!ELEMENT person (name, address)>
  <!ELEMENT name (#PCDATA)>
  <!ELEMENT address (street, city, state, zip)>
  <!ELEMENT street (#PCDATA)>
  <!ELEMENT city (#PCDATA)>
  <!ELEMENT state (#PCDATA)>
  <!ELEMENT zip (#PCDATA)>
  <!ATTLIST person id ID #REQUIRED>
]>
<root>
  <person id="1">
    <name>John Smith</name>
    <address>
      <street>123 Main St</street>
      <city>Anytown</city>
      <state>CA</state>
      <zip>12345</zip>
    </address>
  </person>
  <person id="2">
    <name>Jane Doe</name>
    <address>
      <street>456 Elm St</street>
      <city>Othertown</city>
      <state>NY</state>
      <zip>67890</zip>
    </address>
  </person>
</root>