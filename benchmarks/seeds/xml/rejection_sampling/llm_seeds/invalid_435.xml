  <!ELEMENT street (#PCDATA)>
  <!ELEMENT apt (#PCDATA)>
  <!ELEMENT type (#PCDATA)>
  <!ELEMENT number (#PCDATA)>
  <!ELEMENT address2 (#PCDATA)>
]>
<root>
  <person>
    <name>John Doe</name>
    <address>
      <street>123 Main St</street>
