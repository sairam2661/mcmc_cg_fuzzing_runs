<?xml version='1.0' encoding='ISO-8859-1'?>
<!DOCTYPE root [
  <!ELEMENT root (person|place|thing)*>
  <!ELEMENT person (#PCDATA|address|phone|email)*>
  <!ELEMENT place (#PCDATA|city|state|zip|country)*>
  <!ELEMENT thing (#PCDATA|weight|size|color)*>
  <!ELEMENT address (#PCDATA|street|apt|city|state|zip|country)*>
