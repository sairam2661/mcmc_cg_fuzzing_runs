  <!ELEMENT state (#PCDATA)>
  <!ELEMENT zip (#PCDATA)>
  <!ELEMENT country (#PCDATA)>
  <!ELEMENT weight (#PCDATA)>
  <!ELEMENT size (#PCDATA)>
