<?xml version='1.0' encoding='UTF-8' standalone='no'?>
<!DOCTYPE book [
  <!ELEMENT book (author, publisher, copyright, content)>
  <!ELEMENT author (#PCDATA)>
  <!ELEMENT publisher (#PCDATA)>
  <!ELEMENT copyright (#PCDATA)>
  <!ELEMENT content (section, section, section)>
  <!ELEMENT section (title, p, list)>
  <!ELEMENT title (#PCDATA)>
  <!ELEMENT p (#PCDATA)>
  <!ELEMENT list (item, item)>
  <!ELEMENT item (#PCDATA)>
]>
<book>
  <author>John Smith</author>
  <publisher>ABC Publishers</publisher>
  <copyright>2023</copyright>
  <content>
    <section>
      <title>Introduction</title>
      <p>This is the introduction.</p>
      <list>
        <item>Item one</item>
        <item>Item two</item>
      </list>
    </section>
    <section>
      <title>Chapter One</title>
      <p>This is chapter one.</p>
      <list>
        <item>Item one</item>
        <item>Item two</item>
      </list>
    </section>
    <section>
      <title>Conclusion</title>
      <p>This is the conclusion.</p>
      <list>
        <item>Item one</item>
        <item>Item two</item>
      </list>
    </section>
  </content>
</book> 

