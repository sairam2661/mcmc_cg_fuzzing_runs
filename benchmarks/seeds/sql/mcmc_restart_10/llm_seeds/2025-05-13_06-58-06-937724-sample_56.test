set testdir [file dirname $argv0]
source $testdir/tester.tcl
set testprefix fuzztest

set ::timeout 60000

proc fuzz_table {} {
  global fuzz_name fuzz_sql
  set fuzz_name "fuzz_table"
  set fuzz_sql {
    SELECT count(*) FROM fuzz_table;
    INSERT INTO fuzz_table VALUES(42);
    CREATE TABLE fuzz_table(x);
    INSERT INTO fuzz_table SELECT 1;
  }
}

proc fuzz_expr {} {
  global fuzz_name fuzz_sql
  set fuzz_name "fuzz_expr"
  set fuzz_sql {
