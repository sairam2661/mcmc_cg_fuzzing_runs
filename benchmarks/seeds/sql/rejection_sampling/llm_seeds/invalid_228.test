set testdir [file dirname $argv0]
source $testdir/tester.tcl
set testprefix sync

proc sync_proc {} {
  global syncargs
  lappend syncargs
  expr {[llength $syncargs] <= 2}
}

ifcapable!wal {
  finish_test
  return
}

do_test sync-1.1 {
  execsql {
    CREATE TABLE a(x);
    INSERT INTO a VALUES(1),(2),(3);
  }
  execsql {SELECT count(*) FROM a}
} {3}

do_test sync-1.2 {
  db eval {
    SELECT x FROM a
  }
} {1 2 3}

do_test sync-1.3 {
  db eval {
    SELECT x FROM a
  }
} {1 2 3}

ifcapable tempdb {
  do_test sync-1.4 {
    db eval {
      SELECT name FROM sqlite_temp_master
    }
  } {sqlite_temp}
}

finish_test