set testdir [file dirname $argv0]
source $testdir/tester.tcl
set testprefix sync

proc sync_proc {} {
  global syncargs
  lappend syncargs
  expr {[llength $syncargs] <= 2}
}

do_test sync-1.2 {
  execsql {
    CREATE TABLE b(y);
    INSERT INTO b VALUES(2),(3),(4);
  }
  execsql {SELECT count(*) FROM b}
} {3}

ifcapable journalMode {
  catchsql {PRAGMA journal_mode=WAL}
} {0 {}}

finish_test